<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/css/user.css" />
</head>

<body>
    <%- include('includes/navigation.ejs') %>
 

    <main>
        <% if(users.length>0){ %>
        <div class="grid">
            <ul  >
          <% for (let user of users){%>
  
          <article class="card product-item">
              <li ><%=user.name%></li>           
          </article>
  
          <% } %>
        </div>
    </ul>
        <% }else{ %>
        <h1>users not found</h1>
  
        <% } %>
      </main>

<%- include('includes/end.ejs') %>

   
